<h1>New Item</h1>
<%= form_tag "/merchant/items" do %>
  <%= label_tag :name %>
  <%= text_field_tag :name %>

  <%= label_tag :description %>
  <%= text_field_tag :description %>

  <%= label_tag :price %>
  <%= text_field_tag :price %>

  <%= label_tag :image %>
  <%= url_field_tag :image, @item.image %>

  <%= label_tag :inventory %>
  <%= text_field_tag :inventory %>

  <%= submit_tag 'Create Item' %>
<% end %>